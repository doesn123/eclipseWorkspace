import java.util.Random;

public class Main {
	
	static char[] sticking = {'R', 'L','r','l'};

	public static void main(String[] args) {
		Random r = new Random();
		int ranNum = r.nextInt(sticking.length);
		reverse();
		random();
	
	}
	
	private static void random() {
		int x = 0; 
		while(x <= sticking.length) {
			System.out.println(sticking[ranNum]);
		}
		
	}

	public static void reverse() {
		for(int i = sticking.length - 1; i >= 0; i--) {
			System.out.print(sticking[i]);
		}
	}
}



	


