import java.util.Random;

public class Main {
	
	static char[] sticking = {'R', 'L','r','l'};
	static Random r = new Random();
	static int ranNum = r.nextInt(sticking.length);

	public static void main(String[] args) {
		reverse();
		random();
	
	}
	
	public static void random() {
		int x = 0; 
		while(x <= sticking.length) {
			System.out.println(sticking[ranNum]);
			x++;
		}
	}

	public static void reverse() {
		for(int i = sticking.length - 1; i >= 0; i--) {
			char strokeOne = sticking[i];
			System.out.println(strokeOne);
		}
	}
}



	


